//student grade
// Online C compiler to run C program online
#include <stdio.h>

int main() {
 int x;
 scanf("%d",&x);
 switch(x/10){
     case 10:
     case 9:
     printf("A");
     break;
     case 8:
     printf("B");
     break;
     case 7:
     printf("C");
     break;
     case 6:
     printf("D");
     break;
     default:
     printf("F");
 }
    return 0;
}
//leap year 
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    // Write C code here
    int x;
    scanf("%d",&x);
    if(x%400==0){
        printf("YES");
}else if(x%100==0){
        printf("NO");
    }else if(x%4==0){
        printf("YES");
    }else{
        printf("NO");
    }

    return 0;
}
//vowel
// Online C compiler to run C program online
#include <stdio.h>

int main() {
   char a;
   scanf("%c",&a);
   if(a=='a'||a=='e'||a=='e'||a=='i'||a=='o'||a=='u'){
       printf("YES");
   }else{
       printf("NO");
   }

    return 0;
}
//signal light
// Online C compiler to run C program online
#include <stdio.h>

int main() {
   char a;
   scanf("%c",&a);
   if(a=='r')
   printf("g");
   else if(a=='g')
   printf("y");
   else
   printf("r");

    return 0;
}
//pattern
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n;
    scanf("%d",&n);
    for(int i=0;i<=n;i++){
        for(int j=0;j<i;j++){
            printf("*");
        }
    
        printf("\n");
    }
    

    return 0;
}
//perfect number
// Online C compiler to run C program online
#include <stdio.h>

int main() {
    int n,s=0;
    scanf("%d",&n);
    for(int i=1;i<n;i++){
        if(n%i==0)
        s=s+i;
    }
    if(s==n)
    printf("yes");
    
    return 0;
}
//distance cordiantes 
// Online C compiler to run C program online
#include <stdio.h>
#include <math.h>
int main() {
    float x1,y1,x2,y2;
    scanf("%f%f%f%f",&x1,&y1,&x2,&y2);
    int result=((x2-x1)*(x2-x1))+((y2-y1)*(y2-y1));
    printf("%.4f",sqrt(result));

    return 0;
}
