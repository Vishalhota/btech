// 1st simple calculator 
#include <stdio.h>
int add(int a,int b){
    return a+b;
}
int mo(int a,int b){
    return a*b;
}
int su(int a,int b){
    return a-b;
    
}
int divi(int a,int b){
    return a%b;
}
int main() {
  int a,b,r;
  char c;
  scanf("%d %d %c",&a,&b,&c);
if(c=='a')
r=add(a,b);
else if(c=='m')
r=mo(a,b);
printf("%d",r);
    return 0;
}
//factorial
// Online C compiler to run C program online
#include <stdio.h>
int factorial(int n){
   if (n==1) 
   return 1;
   else
   
    return n*factorial(n-1);
}
int main() {
    // Write C code here
    int a;
    scanf("%d",&a);
    int result=factorial(a);
    printf("%d",result);

    return 0;
}
//2nd
// Online C compiler to run C program online
#include <stdio.h>

int main() {
 int n,t=9,s=0;
 scanf("%d",&n);
 for(int i=0;i<n;i++){
     s+=t;
     
     t=t*10+9;
 }
printf("%d ",s);
    return 0;
}
